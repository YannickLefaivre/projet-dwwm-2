for (let i = 1; i <= 5; i++) {
    class MyElement extends HTMLElement {
        constructor() {
            super();
            this._shadowRoot = this.attachShadow({ mode: 'open' });
            this._shadowRoot.innerHTML = `<div class="rectangle">
              <p></p>
            </div>`;
            this._shadowRoot.querySelector("p").innerHTML = `Dynamic Data ${i}`;
        }
    }
    customElements.define(`my-element${i}`, MyElement);
    const newElement = document.createElement(`my-element${i}`);
    document.body.appendChild(newElement);
  }
  